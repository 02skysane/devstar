{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"outDir": "./dist",
		"ignoreDeprecations": ["importsNotUsedAsValues"],
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true
	},
	"include": [
		"src/**/*.ts",
		"src/**/*.js",
		"src/**/*.svelte"
	]
}